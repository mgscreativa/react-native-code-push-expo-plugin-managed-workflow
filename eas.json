{
  "cli": {
    "version": ">= 3.18.3"
  },
  "build": {
    "base": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "cache": {
        "key": "1.0.0.4"
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "env": {
          "GOOGLE_SERVICES_FILE": "expo.dev"
        }
      }
    },
    "production": {
      "extends": "base",
      "android": {
        "buildType": "app-bundle",
        "env": {
          "GOOGLE_SERVICES_FILE": "expo.dev"
        }
      }
    },
    "production-android-apk": {
      "extends": "production",
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {}
  }
}
