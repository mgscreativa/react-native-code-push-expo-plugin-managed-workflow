{
  "cli": {
    "version": ">= 7.0.0"
  },
  "build": {
    "base": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "env": {
          "GOOGLE_SERVICES_FILE": "expo.dev"
        }
      },
      "cache": {
        "key": "1.0.0.5"
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "development-ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "production-ios-simulator": {
      "extends": "production",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "extends": "base",
      "android": {
        "buildType": "app-bundle"
      }
    },
    "production-preview": {
      "extends": "base",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
